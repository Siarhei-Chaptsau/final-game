    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "0.1.19",
    "cross-env": "5.1.6",
    "css-loader": "0.28.11",
    "file-loader": "1.1.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "3.2.0",
    "style-loader": "0.21.0",
    "url-loader": "1.0.1",
    "webpack": "4.11.1",
    "webpack-cli": "3.0.2",
